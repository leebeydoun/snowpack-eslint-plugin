    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.0.5",
    "snowpack": "^3.0.11",
    "typescript": "^4.1.5"
